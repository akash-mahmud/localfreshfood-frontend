{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next",
    "prettier",
    "next/core-web-vitals",
    "plugin:react/recommended",
    "standard-with-typescript",
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json",
    
    "ecmaFeatures": {
      "jsx": true
    }

  },
  "plugins": ["prettier", "react"],
  "rules": { "@next/next/no-img-element": "off", "require-jsdoc": 0 , 
  "space-before-function-paren":"off",
  "no-extra-semi":"off",  "quote-props":"off", "max-len":"off"},
  "settings": {
    "react": {
      "version": "18.2.0"
    }
  }
}
